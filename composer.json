{
    "name": "mina-amir1/laravel-gettext",
    "description": "Adds localization support to laravel 9 applications in an easy way using Poedit and GNU gettext.",
    "homepage": "https://github.com/mina-amir1/laravel-gettext",
    "keywords": [
        "gettext",
        "localization",
        "poedit",
        "laravel-gettext",
        "laravel", "translation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "NicolÃ¡s Daniel Palumbo",
            "email": "n@xinax.net"
        },
        {
            "name": "Mina Amir",
            "email": "minaamirsobhyamin@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/mina-amir1/laravel-gettext/issues"
    },
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer" : "^3.0",
        "laravel/laravel": "^9.0"
    },
    "autoload": {
        "psr-0": {
            "Xinax\\LaravelGettext\\": "src/"
        },
        "files": [
            "src/Xinax/LaravelGettext/Support/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Xinax\\LaravelGettext\\LaravelGettextServiceProvider"
            ],
            "aliases": {
                "LaravelGettext": "Xinax\\LaravelGettext\\Facades\\LaravelGettext"
            }
        }
    }
}
